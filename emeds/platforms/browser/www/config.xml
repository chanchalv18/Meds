<?xml version='1.0' encoding='utf-8'?>
<widget id="com.whitehats.emeds" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Emeds</name>
    <description>
        A prescription digitization system and misc other benefits.
    </description>
    <author email="sukhatmegaurav@gmail.com" href="https://linkedin.com/in/gaurav-sukhatme-8926b0165/">
        Team White Hats
    </author>
    <content src="index.html" />
    <access origin="*" subdomains="true" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="20" />
    <preference name="android-targetSdkVersion" value="23" />
    <preference name="FadeSplashScreen" value="true" />
    <preference name="FadeSplashScreenDuration" value="3000" />
</widget>
